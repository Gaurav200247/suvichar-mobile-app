{
  "expo": {
    "name": "Suvichar",
    "slug": "suvichar-daily-motivation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "scheme": "suvichar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/app_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.suvichar.dailymotivation"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.suvichar.dailymotivation"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/app_icon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
